package Lista1205;

public class Ex_01_Controle 
{
	public static void main(String[] args) 
	{
		Ex_01_Cachorro[] Lista01=new Ex_01_Cachorro[2];
		Lista01[0]=new Ex_01_Cachorro("Trakina",12,"Auuuu Auuuu");
		Lista01[1]=new Ex_01_Cachorro("Lobinha",8,"Au Au");
				
		//For it
		for(Ex_01_Cachorro Roda:Lista)
		{
			Roda.ImprimirInfo();
		}

		System.out.println("****************************************");
		Ex_01_Cavalo[] Lista02=new Ex_01_Cavalo[2];
		Lista02[0]=new Ex_01_Cavalo("Princesa",5,"Riiiiin");
		Lista02[1]=new Ex_01_Cavalo("Alazão",4,"Rin Riiin");
				
		//For it
		for(Ex_01_Cavalo Roda:Lista)
		{
			Roda.ImprimirInfo();
		}
		
	}

}
